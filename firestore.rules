rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /users/{document=**} {
      allow read, write: if request.auth != null;
    }
    match /reservations/{document=**} {
      allow read, write: if request.auth != null;
    }
    match /secret-code/{document=**} {
      allow read: if true;
    }
  }
}