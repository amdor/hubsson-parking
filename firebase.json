{
	"hosting": {
		"public": "dist/hubsson-parking",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [
			{
				"source": "**",
				"destination": "/index.html"
			}
		]
	},
	"emulators": {
		"hosting": {
			"port": 5000
		},
		"ui": {
			"enabled": true
		},
		"firestore": {
			"port": 8080,
			"rules": "./firestore.rules"
		},
		"auth": {
			"port": 9099
		}
	}
}
